{
    "title": "Song",
    "description": "The song schema represents an Song",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "artist": {
            "type": "integer",
            "foreignSchema": "Artist"
        },
        "album": {
            "type": "integer",
            "foreignSchema": "Album"
        },
        "genre": {
            "type": "integer",
            "foreignSchema": "Genre"
        }
    },
    "required": ["name"]
}